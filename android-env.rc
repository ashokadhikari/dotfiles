# Add environment variable ANDROID_SDK_ROOT
export ANDROID_SDK_ROOT=$HOME/projects/android/android-sdk-macosx
export PATH=$ANDROID_SDK_ROOT:$ANDROID_SDK_ROOT/tools:$ANDROID_SDK_ROOT/platform-tools:$PATH
