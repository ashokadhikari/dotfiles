# Add environment variable ANT_ROOT
export ANT_ROOT=/usr/local/bin
export PATH=$ANT_ROOT:$PATH

# Add environment variable NDK_ROOT
export NDK_ROOT=$HOME/projects/android/android-ndk-r10e
